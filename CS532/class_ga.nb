    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"children", ",", "newPopulation"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"children", "=", 
        RowBox[{"GenerateChildren", "[", 
         RowBox[{"population", ",", "nChildren"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"newPopulation", "=", 
        RowBox[{"Join", "[", 
         RowBox[{"population", ",", "children"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"newPopulation", "=", 
        RowBox[{"TournamentSurvival", "[", 
         RowBox[{"newPopulation", ",", 
          RowBox[{"Length", "[", "population", "]"}]}], "]"}]}]}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"GenerateChildren", "[", 
     RowBox[{"population_", ",", "nChildren_"}], "]"}], ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"parentPair", ",", "childrenPair", ",", "children"}], "}"}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"children", "=", 
        RowBox[{"Table", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"parentPair", "=", 
            RowBox[{"RandomSample", "[", 
