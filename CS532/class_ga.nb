           "\[IndentingNewLine]", 
           RowBox[{"childrenPair", "=", 
            RowBox[{"OnePointCrossover", "[", 
             RowBox[{
              RowBox[{"parentPair", "[", 
               RowBox[{"[", "1", "]"}], "]"}], ",", 
              RowBox[{"parentPair", "[", 
               RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
